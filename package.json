{
  "name": "topwr-api",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#enums/*": "./app/enums/*.js",
    "#scopes/*": "./app/scopes/*.js",
    "#utils/*": "./app/utils/*.js",
    "#decorators/*": "./app/decorators/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#commands/*": "./commands/*.js"
  },
  "scripts": {
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "prepare": "husky || true",
    "prettiercheck": "prettier --check .",
    "start": "node bin/server.js",
    "test": "node ace test",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "prettier": "@solvro/config/prettier",
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "dependencies": {
    "@adminjs/adonis": "^1.1.0",
    "@adminjs/passwords": "^4.0.0",
    "@adminjs/relations": "^1.1.2",
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/drive": "^3.2.0",
    "@adonisjs/hash": "^9.0.5",
    "@adonisjs/limiter": "^2.3.3",
    "@adonisjs/lucid": "^20.0.0",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/static": "^1.1.1",
    "@solvro/solvronis-metrics": "^1.0.0",
    "@solvro/utils": "^1.1.2",
    "@vinejs/vine": "^2.1.0",
    "adminjs": "^7.8.13",
    "argon2": "^0.41.1",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@commitlint/cli": "^19.6.1",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@solvro/config": "^1.13.5",
    "@swc/core": "1.7.26",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.11",
    "eslint": "^9.12.0",
    "hot-hook": "^0.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.6"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  },
  "hotHook": {
    "boundaries": [
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  }
}
